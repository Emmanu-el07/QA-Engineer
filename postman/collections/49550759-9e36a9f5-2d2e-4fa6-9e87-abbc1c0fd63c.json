							"// Validate the Status Code\r",
							"pm.test(\"Status Code is 200\",()=>{\r",
							"\tpm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"// Validate for Multiple response Code\r",
							"pm.test(\"Successful GET request\",()=>{\r",
							"\tpm.expect(pm.response.code).to.be.oneOf([200,201]);\r",
							"});\r",
							"\r",
							"// Validate for String type Status Code\r",
							"pm.test(\"Status code is a string\", () => {\r",
							"\tpm.response.to.have.status(\"OK\");\r",
							"});\r",
							"\r",
							"// Check that a response header is present\r",
							"pm.test(\"Response header is present\", () => {\r",
							"\tpm.response.to.have.header(\"Content-Type\");\r",
							"});\r",
							"\r",
							"// Test for response header having a specific response\r",
							"pm.test(\"Content-Type header is application/json\",() =>{\r",
